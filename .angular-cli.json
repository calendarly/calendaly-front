{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
        "name": "material"
    },
    "apps": [{
        "root": "src",
        "outDir": "dist",
        "assets": [
            "assets",
            "favicon.ico"
        ],
        "index": "index.html",
        "main": "main.ts",
        "polyfills": "polyfills.ts",
        "test": "test.ts",
        "tsconfig": "tsconfig.app.json",
        "testTsconfig": "tsconfig.spec.json",
        "prefix": "app",
        "styles": [
            "../node_modules/bootstrap/dist/css/bootstrap.min.css",
            "../node_modules/font-awesome/css/font-awesome.min.css",
            "../node_modules/animate.css/animate.min.css",
            "../node_modules/summernote/dist/summernote-lite.css",
            "../node_modules/material-design-iconic-font/dist/css/material-design-iconic-font.min.css",
            "../node_modules/sweetalert2/dist/sweetalert2.css",
            "../node_modules/dragula/dist/dragula.css",
            "assets/vendors/fullcalendar/fullcalendar.css",
            "assets/css/snackbar.css",
            "styles.scss"
        ],
        "scripts": [
            "assets/js/jquery-2.1.1.min.js",
            "assets/js/bootstrap.min.js",
            "../node_modules/summernote/dist/summernote-lite.js",
            "../node_modules/sweetalert2/dist/sweetalert2.all.js",
            "assets/vendors/fullcalendar/lib/moment.min.js",
            "assets/vendors/fullcalendar/fullcalendar.min.js",
            "assets/js/snackbar.js"
        ],
        "environmentSource": "environments/environment.ts",
        "environments": {
            "dev": "environments/environment.ts",
            "prod": "environments/environment.prod.ts"
        }
    }],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "lint": [{
            "project": "src/tsconfig.app.json",
            "exclude": "**/node_modules/**"
        },
        {
            "project": "src/tsconfig.spec.json",
            "exclude": "**/node_modules/**"
        },
        {
            "project": "e2e/tsconfig.e2e.json",
            "exclude": "**/node_modules/**"
        }
    ],
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "scss",
        "component": {}
    }
}