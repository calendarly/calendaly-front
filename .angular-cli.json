{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
        "name": "material"
    },
    "apps": [{
        "root": "src",
        "outDir": "dist",
        "assets": [
            "assets",
            "favicon.ico"
        ],
        "index": "index.html",
        "main": "main.ts",
        "polyfills": "polyfills.ts",
        "test": "test.ts",
        "tsconfig": "tsconfig.app.json",
        "testTsconfig": "tsconfig.spec.json",
        "prefix": "app",
        "styles": [
            "../node_modules/bootstrap/dist/css/bootstrap.min.css",
            "../node_modules/font-awesome/css/font-awesome.min.css",
            "../node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
            "../node_modules/material-design-iconic-font/dist/css/material-design-iconic-font.min.css",
            "assets/vendors/socicon/socicon.min.css",
            "../node_modules/animate.css/animate.min.css",
            "../node_modules/summernote/dist/summernote-lite.css",
            "../node_modules/dropzone/dist/min/dropzone.min.css",
            "../node_modules/sweetalert2/dist/sweetalert2.css",
            "../node_modules/nouislider/distribute/nouislider.min.css",
            "../node_modules/bootstrap-tagsinput/dist/bootstrap-tagsinput.css",
            "../node_modules/dragula/dist/dragula.css",
            "assets/vendors/fullcalendar/fullcalendar.css",
            "styles.scss"
        ],
        "scripts": [
            "assets/js/jquery-2.1.1.min.js",
            "assets/js/bootstrap.min.js",
            "../node_modules/select2/dist/js/select2.full.min.js",
            "../node_modules/jquery-sparkline/jquery.sparkline.min.js",
            "../node_modules/easy-pie-chart/dist/jquery.easypiechart.min.js",
            "../node_modules/dropzone/dist/dropzone.js",
            "../node_modules/nouislider/distribute/nouislider.min.js",
            "../node_modules/flot/jquery.flot.js",
            "../node_modules/summernote/dist/summernote-lite.js",
            "../node_modules/flot/jquery.flot.pie.js",
            "../node_modules/bootstrap-tagsinput/dist/bootstrap-tagsinput.js",
            "../src/assets/vendors/bower_components/flot.curvedlines/curvedLines.js",
            "../src/assets/vendors/bower_components/flot.orderbars/js/jquery.flot.orderBars.js",
            "../node_modules/jqvmap/dist/jquery.vmap.min.js",
            "../node_modules/sweetalert2/dist/sweetalert2.all.js",
            "../node_modules/jqvmap/dist/maps/jquery.vmap.world.js",
            "assets/vendors/fullcalendar/lib/moment.min.js",
            "assets/vendors/fullcalendar/fullcalendar.min.js",
            "assets/vendors/bootstrap-datetimepicker/bootstrap-datetimepicker.min.js"

        ],
        "environmentSource": "environments/environment.ts",
        "environments": {
            "dev": "environments/environment.ts",
            "prod": "environments/environment.prod.ts"
        }
    }],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "lint": [{
            "project": "src/tsconfig.app.json",
            "exclude": "**/node_modules/**"
        },
        {
            "project": "src/tsconfig.spec.json",
            "exclude": "**/node_modules/**"
        },
        {
            "project": "e2e/tsconfig.e2e.json",
            "exclude": "**/node_modules/**"
        }
    ],
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "scss",
        "component": {}
    }
}